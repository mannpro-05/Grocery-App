<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.guni.uvpce.ceit.food">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/foodlogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/foodlogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".SupplierList"
            android:parentActivityName=".activity.DashboardActivity"/>
        <activity android:name=".EditSupplier"
            android:parentActivityName=".activity.DashboardActivity"/>
        <activity android:name=".AddSupplier"
            android:parentActivityName=".activity.DashboardActivity"/>
        <activity
            android:name=".activity.EditItems"
            android:parentActivityName=".activity.DashboardActivity" />
        <activity
            android:name=".DesignByActivity"
            android:parentActivityName=".activity.DashboardActivity" />
        <activity
            android:name=".activity.AddAdminActivity"
            android:parentActivityName=".activity.DashboardActivity" />
        <activity
            android:name=".activity.AdminOrderHistoryActivity"
            android:parentActivityName=".activity.DashboardActivity" />
        <activity
            android:name=".activity.SingleOrderHistoryActivity"
            android:parentActivityName=".activity.OrderHistoryActivity" />
        <activity
            android:name=".activity.SetAvailabilityActivity"
            android:parentActivityName=".activity.DashboardActivity" />
        <activity
            android:name=".activity.AddItemsActivity"
            android:parentActivityName=".activity.DashboardActivity" />
        <activity
            android:name=".activity.OrderHistoryActivity"
            android:parentActivityName=".activity.DashboardActivity" />
        <activity
            android:name=".activity.CartActivity"
            android:parentActivityName=".activity.DashboardActivity" />
        <activity
            android:name=".activity.EditProfileActivity"
            android:parentActivityName=".activity.DashboardActivity" />
        <activity
            android:name=".activity.ProfileActivity"
            android:parentActivityName=".activity.DashboardActivity" />
        <activity android:name=".activity.DashboardActivity" />
        <activity
            android:name=".activity.RegisterActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".activity.ResetPasswordActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.theartofdev.edmodo.cropper.CropImageActivity"
            android:theme="@style/Base.Theme.AppCompat" />
    </application>

</manifest>